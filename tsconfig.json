{
  "compilerOptions": {
    "declaration": true,
    "moduleResolution": "node",
    "module": "es2015",
    "target": "es2015",
    "lib": [
      "dom",
      "es5",
      "es2015"
    ],
    "noImplicitAny": true,
    "sourceMap": true,
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "strictNullChecks": true,
    "outDir": "lib/es2015",
    "types": [
    ]
  },
  "include": [
    "custom-typings/*.d.ts",
    "src/index.ts"
  ]
}
